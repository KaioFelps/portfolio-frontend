{
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{
		"files": "*.svelte",
		"options": {
			"parser": "svelte"
		}
	}],
	"useTabs": true,
	"printWidth": 100,
	"endOfLine": "auto",
	"singleQuote": false,
	"jsxSingleQuote": false,
	"tabWidth": 2,
	"trailingComma": "all",
	"arrowParens": "always",
	"semi": true,
	"svelteBracketNewLine": true,
	"bracketSpacing": true,
	"bracketSameLine": false,"htmlWhitespaceSensitivity": "strict"
}